{
  "name": "personal-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "optimize": "npx avif --input='./public/**/*.{jpg,jpeg,tif,tiff,webp,png,gif}' --overwrite --verbose --effort=9 --quality=80",
    "dev": "npm run optimize && vite",
    "build": "npm run optimize && vite build",
    "preview": "vite preview",
    "check": "npm run optimize && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
