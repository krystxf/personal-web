{
	"name": "personal-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"optimize": "npx avif --input='./static/**/*.{jpg,jpeg,tif,tiff,webp,png,gif}' --overwrite --verbose --effort=9 --quality=80",
		"prebuild": "npm run optimize",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.21",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"heroicons-for-svelte": "^0.0.10"
	}
}
